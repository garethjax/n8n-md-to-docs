{
  "name": "markdown-to-gdocs-function",
  "version": "1.1.0",
  "description": "Secure Firebase Function to convert Markdown to Google Docs with comprehensive security measures",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./lib --target node && cp package.json lib/",
    "build:watch": "bun build ./src/index.ts --outdir ./lib --target node --watch",
    "serve": "bun run build && firebase emulators:start",
    "shell": "bun run build && firebase functions:shell",
    "start": "bun run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "dev": "bun --watch src/index.ts",
    "security-audit": "npm audit",
    "security-test": "node security-test.js",
    "type-check": "npx tsc --noEmit"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "docx": "^8.5.0",
    "express": "^4.18.2",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^4.5.0",
    "googleapis": "^128.0.0",
    "marked": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/marked": "^5.0.2",
    "@types/node": "^18.0.0",
    "bun-types": "latest",
    "firebase-functions-test": "^3.1.0"
  },
  "keywords": [
    "n8n",
    "markdown",
    "google-docs",
    "firebase",
    "security",
    "converter"
  ],
  "private": true
}
